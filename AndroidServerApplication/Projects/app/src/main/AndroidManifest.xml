<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tutorials.coderzheaven.com.androidtutorialprojects">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CameraPermissionActivity"></activity>
        <activity
            android:name="com.androidtutorialprojects.SelectImageActivity"
            android:label="@string/title_activity_select_image"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity
            android:name="store_image_in_sqlite.StoreImageActivity"
            android:label="@string/title_activity_select_image"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>